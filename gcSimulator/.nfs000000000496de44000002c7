mv /data/remy/fop.trace /data/remy/temp.trace &&
cp gen.hs Main.hs && /usr/bin/time -o timing.temp cabal run && mv timing.temp gentiming.log &&
cp bangs.hs Main.hs && /usr/bin/time -o timing.temp cabal run && mv timing.temp bangstiming.log &&
cp nobangs.hs Main.hs && /usr/bin/time -o timing.temp cabal run && mv timing.temp nobangstiming.log &&
mkdir fop && mv *timing* fop &&
mv /data/remy/temp.trace /data/remy/fop.trace 

